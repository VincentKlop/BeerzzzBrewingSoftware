{% extends 'base.html.twig' %}

{% block page_title %}Ingredient Type{% endblock %}
{% block page_subtitle %}configuration{% endblock %}

{% block page_content %}
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ingredient_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ingredient_type.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ingredient_type_index') }}"><i class="fas fa-list"></i></a>

    <a href="{{ path('ingredient_type_edit', {'id': ingredient_type.id}) }}"><i class="fas fa-edit"></i></a>

    {{ include('ingredient_type/_delete_form.html.twig') }}
{% endblock %}
